                        <a href="@Url.Action("TrainingPlan", "Home")">Level 1</a>
                        <a href="@Url.Action("TrainingPlan2", "Home")">Level 2</a>

                    </div>
                </div>
                <div class="dropdown">
                    <a href="#" class="dropbtn">
                        Quizzes <span class="arrow">&#9660;</span>
                    </a>
                    <div class="dropdown-content">
